@{
    Layout = null;


    var seriesTitle = "Te Amazing Spiderman";
    var issueNumber = 700;
    var description = "Spiderman does something cool";
    var artists = new string[]
        {
            "Yeers",
            "Yoors",
            "Meemers",
        };
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
        <h1> @seriesTitle</h1>
        
        <h2>Issue #@issueNumber</h2>

        <h5> Description:</h5>
        <div>  @Html.Raw(description)</div>

        <h5> Artists:</h5>
        <div>@artists</div>
    </div>
</body>
</html>
